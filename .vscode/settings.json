{
  "editor.quickSuggestions": {
    "other": "on",
    "comments": true,
    "strings": "off"
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.lintTask.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.words": [
    "autoincrement",
    "Chatbots"
  ]
}