{
    "editor.quickSuggestions": {
      "other": "on",
      "comments": true,
      "strings": "off"
    },
    "eslint.enable": true,
    "eslint.nodePath": "./node_modules",
    "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
    ],
    "eslint.validate": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "typescript.tsdk": "node_modules/typescript/lib",
    "cSpell.words": [
      "autoincrement",
      "Chatbots"
    ]
}